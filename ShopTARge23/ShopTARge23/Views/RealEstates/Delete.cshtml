@model ShopTARge23.Models.RealEstates.RealEstateDeleteViewModel

<!-- Main -->
<div id="main" class="wrapper style1">
    <div class="container">
        <header class="major">
            <h2>Delete</h2>
            <p>Real Estate</p>
        </header>

        <!-- Content -->
        <section id="content">
            <form method="post">
                <table class="table table-hover">
                    <tr>
                        <td><label asp-for="Id" class="col-sm-2 col-form-label"></label></td>
                        <td>@Html.DisplayFor(model => model.Id)</td>
                    </tr>
                    <tr>
                        <td><label as<label asp-for="Size" class="col-sm-2 col-form-label"></label></td>
                        <td> @Html.DisplayFor(model => model.Size)</td>
                    </tr>
                    <tr>
                        <td><label asp-for="Location" class="col-sm-2 col-form-label"></label></td>
                        <td> @Html.DisplayFor(model => model.Location)</td>
                    </tr>
                    <tr>
                        <td> <label asp-for="RoomNumber" class="col-sm-2 col-form-label"></label></td>
                        <td>@Html.DisplayFor(model => model.RoomNumber)</td>
                    </tr>
                    <tr>
                        <td>  <label asp-for="BuildingType" class="col-sm-2 col-form-label"></label></td>
                        <td>@Html.DisplayFor(model => model.BuildingType)</td>
                    </tr>
                    <tr>
                        <td><label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label></td>
                        <td>@Html.DisplayFor(model => model.CreatedAt)</td>
                    </tr>
                    <tr>
                        <td><label asp-for="ModifiedAt" class="col-sm-2 col-form-label"></label></td>
                        <td>@Html.DisplayFor(model => model.ModifiedAt)</td>
                    </tr>
                </table>
            </form>

            <div>
                @foreach (var images in Model.Image)
                {
                    <partial name="_Images" model="images" />
                    <br />
                }
                <form asp-action="DeleteConfirmation">
                    <input type="hidden" asp-for="Id" />
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>
                <a asp-action="Index" asp-controller="realEstates" class="btn btn-outline-dark">Back</a>
            </div>
        </section>
    </div>
</div>
